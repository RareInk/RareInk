{
  "name": "rare-ink",
  "version": "1.0.0",
  "description": "Open Source for the RareInk SPA",
  "main": "./dist/server/bin/www.js",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod --sm=false --aot --output-path=dist/client && npm run server:build",
    "e2e:client": "ng e2e",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "now-build": "npm run build",
    "now-start": "node",
    "server:build": "tsc -p ./server",
    "server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon --delay 5 dist/server/bin/www.js\" ",
    "start": "concurrently --kill-others \"npm run server:run\" \"ng serve --aot --progress=false --proxy-config proxy.conf.json\"",
    "test": "npm run test:client",
    "test:client": "ng test"
  },
  "author": "Mhyre",
  "contributors": [
    {
      "name": "Mhyre",
      "url": "https://github.com/eryhM"
    },
    {
      "name": "Resi Respati",
      "email": "resir014@blvd.space",
      "url": "https://github.com/resir014"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eryhM/RareInk.git"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "body-parser": "~1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.3.0",
    "ngrx-store-freeze": "^0.1.9",
    "normalize.css": "^5.0.0",
    "path": "^0.12.7",
    "rxjs": "^5.4.2",
    "sequelize": "^4.8.0",
    "tslint": "^5.7.0",
    "uuid": "^3.0.1",
