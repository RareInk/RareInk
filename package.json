    "@types/jasmine": "2.5.38",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/mongodb": "^2.2.11",
    "@types/node": "~6.0.60",
    "@types/sequelize": "^4.0.72",
    "@types/uuid": "^2.0.29",
    "codelyzer": "~3.1.2",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.5.0",
